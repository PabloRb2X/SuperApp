# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SuperApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SuperApp
  pod 'RealmSwift'
  pod 'Alamofire'
  #pod 'AlamofireImage'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'Firebase/Core'

  target 'SuperAppTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RealmSwift'
    pod 'Alamofire'
    #pod 'AlamofireImage'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'Firebase/Core'
  end

  target 'SuperAppUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RealmSwift'
    pod 'Alamofire'
    #pod 'AlamofireImage'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'Firebase/Core'
  end

end
